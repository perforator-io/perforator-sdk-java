<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">
    <servers>
        <server>
            <id>gitlab-maven</id>
            <configuration>
                <httpHeaders>
                    <property>
                        <name>Job-Token</name>
                        <value>${env.CI_JOB_TOKEN}</value>
                    </property>
                </httpHeaders>
            </configuration>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>loadGenerator.apiBaseUrl.invalid</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <loadGenerator.apiBaseUrl>Please export LOADGENERATOR_APIBASEURL via environment variables</loadGenerator.apiBaseUrl>
            </properties>
        </profile>
        <profile>
            <id>loadGenerator.apiBaseUrl.env</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>env.LOADGENERATOR_APIBASEURL</name>
                </property>
            </activation>
            <properties>
                <loadGenerator.apiBaseUrl>${env.LOADGENERATOR_APIBASEURL}</loadGenerator.apiBaseUrl>
            </properties>
        </profile>
        <profile>
            <id>loadGenerator.apiBaseUrl.sys</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>loadGenerator.apiBaseUrl</name>
                </property>
            </activation>
            <properties>
                <loadGenerator.apiBaseUrl>${loadGenerator.apiBaseUrl}</loadGenerator.apiBaseUrl>
            </properties>
        </profile>
        <profile>
            <id>loadGenerator.apiClientId.invalid</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <loadGenerator.apiClientId>Please export LOADGENERATOR_APICLIENTID via environment variables</loadGenerator.apiClientId>
            </properties>
        </profile>
        <profile>
            <id>loadGenerator.apiClientId.env</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>env.LOADGENERATOR_APICLIENTID</name>
                </property>
            </activation>
            <properties>
                <loadGenerator.apiClientId>${env.LOADGENERATOR_APICLIENTID}</loadGenerator.apiClientId>
            </properties>
        </profile>
        <profile>
            <id>loadGenerator.apiClientId.sys</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>loadGenerator.apiClientId</name>
                </property>
            </activation>
            <properties>
                <loadGenerator.apiClientId>${loadGenerator.apiClientId}</loadGenerator.apiClientId>
            </properties>
        </profile>
        <profile>
            <id>loadGenerator.apiClientSecret.invalid</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <loadGenerator.apiClientSecret>Please export LOADGENERATOR_APICLIENTSECRET via environment variables</loadGenerator.apiClientSecret>
            </properties>
        </profile>
        <profile>
            <id>loadGenerator.apiClientSecret.env</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>env.LOADGENERATOR_APICLIENTSECRET</name>
                </property>
            </activation>
            <properties>
                <loadGenerator.apiClientSecret>${env.LOADGENERATOR_APICLIENTSECRET}</loadGenerator.apiClientSecret>
            </properties>
        </profile>
        <profile>
            <id>loadGenerator.apiClientSecret.sys</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>loadGenerator.apiClientSecret</name>
                </property>
            </activation>
            <properties>
                <loadGenerator.apiClientSecret>${loadGenerator.apiClientSecret}</loadGenerator.apiClientSecret>
            </properties>
        </profile>
        
        <profile>
            <id>loadGenerator.projectKey.invalid</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <loadGenerator.projectKey>Please export LOADGENERATOR_PROJECTKEY via environment variables</loadGenerator.projectKey>
            </properties>
        </profile>
        <profile>
            <id>loadGenerator.projectKey.env</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>env.LOADGENERATOR_PROJECTKEY</name>
                </property>
            </activation>
            <properties>
                <loadGenerator.projectKey>${env.LOADGENERATOR_PROJECTKEY}</loadGenerator.projectKey>
            </properties>
        </profile>
        <profile>
            <id>loadGenerator.projectKey.sys</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>loadGenerator.projectKey</name>
                </property>
            </activation>
            <properties>
                <loadGenerator.projectKey>${loadGenerator.projectKey}</loadGenerator.projectKey>
            </properties>
        </profile>
    </profiles>
</settings>
